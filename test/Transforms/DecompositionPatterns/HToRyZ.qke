// The FileCheck part of this test only cares about the sequence of operations.
// Correcteness is checked by CircuitCheck.

// CHECK-LABEL: func.func @test
func.func @test(%qubit: !quake.ref) {
  quake.h %qubit : (!quake.ref) -> ()
  // CHECK: quake.ry
  // CHECK-NEXT: quake.z
  return
}